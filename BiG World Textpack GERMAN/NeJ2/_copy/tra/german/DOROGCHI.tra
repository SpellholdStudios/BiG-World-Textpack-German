@0 = ~Wer seid Ihr? Was tut Ihr hier?~
@1 = ~Ich suche den Orog Anführer.~
@2 = ~Ich komme gerade vorbei.~
@3 = ~Ihr habt ihn gefunden. Was wollt Ihr?~
@4 = ~Ich bin gekommen, um Euer Abzeichen zu erhalten.~
@5 = ~Ich schlage vor, dass Ihr Euch verteidigt.~
@6 = ~Ihr werdet nichts bekommen, und Ihr werdet sterben.~
@7 = ~Ich bitte Euch, zu unterscheiden.~
@8 = ~Der Zauberer?! Wo IST er?!~
@9 = ~In diesen Höhlen, genau nördlich von hier.~
@10 = ~Tut mir Leid, das kann ich Euch nicht sagen.~
@11 = ~Ihr! Ihr bleibt hier, während wir ihn töten.~
@12 = ~Ist gut.~
@13 = ~Dann werdet Ihr jetzt sterben.~
@14 = ~Ich denke nicht so.~
@15 = ~Niemand kommt vorbei. Ihr sterbt.~
@16 = ~Wir kümmern uns darum.~
