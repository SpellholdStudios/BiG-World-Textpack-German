@4211 = ~[REVENANT 01] ~ [%tutu_var%REVEN01]
@4212 = ~[REVENANT 02] Muss... rächen...~ [%tutu_var%REVEN02]
@4213 = ~[REVENANT 03] Ich bin gekommen um Euch zu holen.~ [%tutu_var%REVEN03]
@4214 = ~[REVENANT 04] Ihr... müsst... bezahlen...~ [%tutu_var%REVEN04]
@4215 = ~[REVENANT 05] ~ [%tutu_var%REVEN05]
@4216 = ~[REVENANT 06] ~ [%tutu_var%REVEN06]
